#include <stdio.h>


#define DEBUG 0
#define MK     printf("<< MARK-> %d >>\n", __LINE__)  // mark location
#define PI(n)  printf(#n ": %d\n", n)                 // print int value
#define PC(c)  printf(#c ": %c\n", c)                 // print char value
#define FC     printf("%s called\n", __func__)        // function called
#define FR     printf("%s returns\n", __func__)       // function returned


int main () 
{


    return 0;
}



















